{
  "name": "pomi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node build/server.js",
    "develop": "concurrently -p name -c \"blue,red,yellow\" -n \"webpack-server,server,react\" \"yarn dev:server:webpack\" \"yarn dev:server:nodemon\" \"yarn dev:client\"",
    "dev:server:webpack": "yarn webpack:server --watch --env dev",
    "dev:server:nodemon": "rm -f build/server.js && wait-on -l build/server.js && nodemon build/server.js",
    "dev:server": "concurrently -p name -c \"blue,red\" -n \"webpack-server,server\" \"yarn dev:server:webpack\" \"yarn dev:server:nodemon\"",
    "dev:client": "wait-on -l tcp:3001 && node scripts/start.js",
    "build": "yarn build:client && yarn build:server",
    "build:server": "yarn webpack:server --env prod",
    "build:client": "node scripts/build.js",
    "webpack:server": "webpack --config webpack.config.server.js",
    "test": "yarn test:server:ci && yarn test:shared:ci && yarn test:client:ci",
    "generate": "plop",
    "migrate-db": "sh src/server/db/migrations/migrate.sh",
    "reset-dev-db": "yarn migrate-db dev 1 && yarn migrate-db dev",
    "reset-test-db": "yarn migrate-db test 1 && yarn migrate-db test",
    "test:client": "yarn test:client:ci --watch",
    "test:client:ci": "jest --config src/client/jest.config.js",
    "test:server": "yarn test:server:ci --watch",
    "test:server:ci": "jest --config src/server/jest.config.js",
    "test:shared": "yarn test:shared:ci --watch",
    "test:shared:ci": "jest --config src/shared/jest.config.js",
    "killnode": "kill -9 $(lsof -t -i:3001)"
  },
