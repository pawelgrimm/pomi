{
  "name": "pomi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "develop": "concurrently \"yarn run start\" \"nodemon --exec ts-node --project server/tsconfig.json server/index.ts\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate": "plop",
    "migrate-db": "sh server/db/migrations/migrate.sh",
    "test:server": "yarn run prepare-test-db && jest --config server/jest.config.js",
    "prepare-test-db": "dropdb pomi_test -U pomi --if-exists && createdb -U pomi pomi_test && yarn run migrate-db test"
  },
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@reduxjs/toolkit": "^1.2.5",
    "@testing-library/dom": "7.24.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/react-hooks": "3.4.1",
    "@testing-library/user-event": "12.1.6",
    "@types/classnames": "2.2.10",
    "@types/date-fns": "2.6.0",
    "@types/express": "4.17.8",
    "@types/jest": "^24.0.0",
    "@types/node": "14.14.2",
    "@types/pg": "7.14.5",
    "@types/react": "16.9.49",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "5.1.5",
    "axios": "0.20.0",
    "classnames": "2.2.6",
    "comlink": "4.3.0",
    "concurrently": "5.3.0",
    "date-fns": "2.16.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-promise-router": "4.0.1",
    "formik": "2.2.0",
    "formik-material-ui": "3.0.0",
    "knex": "0.21.6",
    "nodemon": "2.0.6",
    "notistack": "1.0.1",
    "pg": "8.3.3",
    "pg-migrator": "1.0.5",
    "plop": "2.7.4",
    "prettier": "2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-query": "2.23.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3",
    "react-simple-timefield": "3.2.3",
    "react-test-renderer": "16.13.1",
    "react-use-comlink": "2.0.1",
    "sass": "1.26.10",
    "supertest": "5.0.0",
    "ts-node": "9.0.0",
    "typescript": "~3.8.2",
    "worker-loader": "3.0.2"
  },
  "proxy": "http://localhost:3001",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "worker-loader!./clockWorker-script": "<rootDir>/src/__mocks__/ClockWorker"
    }
  }
}
